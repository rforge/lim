Flow(B4,CO2)	=	[3,13]*Flow(NB4,NH3)
Flow(B5,CO2)	=	[3,13]*Flow(NB5,NH3)
Flow(B6,CO2)	=	[3,13]*Flow(NB6,NH3)
Flow(B7,CO2)	=	[3,13]*Flow(NB7,NH3)
Flow(B8,CO2)	=	[3,13]*Flow(NB8,NH3)
